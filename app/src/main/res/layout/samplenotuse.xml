<?xml version="1.0" encoding="UTF-8"?>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/rlt_layout_root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="Overdraw">




        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_marginBottom="@dimen/dp_10"
            android:background="@color/white"
            android:elevation="@dimen/dp_2"
            android:paddingBottom="@dimen/dp_10"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <View
                android:id="@+id/view_divider_booking"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@color/black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="parent" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_datetime_value"
                android:layout_width="@dimen/dp_0"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginEnd="@dimen/dp_20"
                app:layout_constraintEnd_toStartOf="@+id/txt_status"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="9:45 AM, 24 August 2021" />

            <!--style="@style/MediumBoldPrimaryColorTextViewStyle"-->

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_vehicle_assign"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_15"
                android:tint="@color/red_btn_bg_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_datetime_value"
                app:srcCompat="@drawable/clock" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_driver_assign"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_10"
                android:tint="@color/red_btn_bg_color"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_datetime_value"
                app:srcCompat="@drawable/sedan" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_material_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginEnd="@dimen/dp_20"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/txt_booking_id"
                app:layout_constraintStart_toStartOf="@+id/txt_datetime_value"
                app:layout_constraintTop_toBottomOf="@+id/txt_datetime_value"
                tools:text="Raw Material" />

           <!-- style="@style/MediumJioBPMediumSecondaryColorTextViewStyle"-->

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_datetime_value2"
                android:layout_width="@dimen/dp_0"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginEnd="@dimen/dp_20"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="@+id/txt_material_type"
                app:layout_constraintStart_toStartOf="@+id/txt_material_type"
                app:layout_constraintTop_toBottomOf="@+id/txt_material_type"
                tools:text="VehicleName" />

            <!--style="@style/MediumRegularPrimaryColorTextViewStyle"-->

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_20"
                android:textColor="@color/main_green_color"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/txt_datetime_value"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/txt_datetime_value"
                tools:text="New Bid" />

            <!--style="@style/LargeBoldPrimaryColorTextViewStyleOne"-->

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_booking_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/img_vehicle_assign"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/img_vehicle_assign"
                tools:text="B0085" />

            <!--style="@style/MediumJioBPMediumSecondaryColorTextViewStyle"-->

            <View
                android:id="@+id/view_divider"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@color/grey_color"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txt_booking_id" />

            <include
                android:id="@+id/include_pick_drop"
                layout="@layout/booking_list_item1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/dp_10"
                android:paddingEnd="@dimen/dp_10"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view_divider" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_view_detail"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp_10"
                android:paddingTop="@dimen/dp_10"
                android:textColor="@android:color/transparent"
                android:textStyle="bold"
                app:drawableEndCompat="@drawable/downa_arrow"
                app:drawableTint="@color/purple_500"
                app:layout_constraintBottom_toBottomOf="@+id/cl_truck_detail"
                app:layout_constraintEnd_toEndOf="parent" />

            <!--style="@style/LargeBoldPrimaryColorTextViewStyleOne"-->
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_truck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/dp_15"
                app:layout_constraintBottom_toBottomOf="@+id/cl_truck_detail"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/cl_truck_detail"
                app:srcCompat="@drawable/calender" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/txt_shipper_name"

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginEnd="@dimen/dp_20"
                android:visibility="gone"
                app:layout_constraintEnd_toStartOf="@+id/txt_booking_id"
                app:layout_constraintStart_toStartOf="@+id/txt_booking_id"
                app:layout_constraintTop_toBottomOf="@+id/txt_booking_id"
                tools:text="VehicleName" />


            <!--style="@style/MediumRegularPrimaryColorTextViewStyle"-->
            -

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_truck_detail"
                android:layout_width="@dimen/dp_0"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/dp_10"
                app:layout_constraintEnd_toStartOf="@+id/txt_view_detail"
                app:layout_constraintStart_toEndOf="@+id/img_truck"
                app:layout_constraintTop_toBottomOf="@+id/include_pick_drop">


                -

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/grey_color"
                    android:paddingHorizontal="@dimen/dp_10"
                    android:paddingVertical="@dimen/dp_5"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_truck_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/package_rules"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <!--style="@style/SmallJioBPMediumPrimaryColorTextViewStyle"-->

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_truck_number_plate"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_5"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:textColor="@color/purple_200"
                        android:visibility="gone"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/txt_truck_type"
                        tools:text="MH14 TY1152" />

                    <!--style="@style/ExtraSmallJioBPMediumPrimaryColorTextViewStyle"-->

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
